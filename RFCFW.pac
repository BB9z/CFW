/*!	RFCFW.pac		*/var DefaultLocalProxyPort = "9000";var P = "PROXY 127.0.0.1:" + DefaultLocalProxyPort + ", DIRECT";var D = "DIRECT";// Host availablevar H = D;function FindProxyForURL(url,host){/* @广谱规则 */	// CN域名直接连接	if(dnsDomainIs(host,".cn"))	return D;/* END @广谱规则 */	// Twitter	if (host == 'twitpic.com')	return P;		// URL Shorter	if (host == 'goo.gl')		return P;	if (host == 'j.mp')			return P;	if (host == 't.co')			return P;	// Video	if (host == 'youtube.com')			return P;	if (host == 'player.vimeo.com')		return P;		// iOS dev	if (host == 'cocoacontrols.com')	return P;		/* @行为检测 */	// 如果域名不能被解析则使用代理	// 不适用于ISP的域名劫持，Windows下推荐使用DnsSpeeder解决劫持问题	if(!isResolvable(host)) return P;/* END @行为检测 *//* @默认行为 */	return P;/* END @默认行为 */}